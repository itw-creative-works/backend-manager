{
  "name": "backend-manager",
  "version": "5.0.84",
  "description": "Quick tools for developing Firebase functions",
  "main": "src/manager/index.js",
  "bin": {
    "bem": "./bin/bem",
    "bm": "./bin/bem"
  },
  "scripts": {
    "start": "node src/manager/index.js",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "node -e \"require('prepare-package/watch')()\""
  },
  "engines": {
    "node": "22"
  },
  "projectScripts": {
    "start": "npx bm setup && npx bm serve",
    "deploy": "npx bm setup && npx bm deploy",
    "emulators": "npx bm setup && npx bm emulators",
    "test": "npx bm setup && npx bm test",
    "setup": "npx bm setup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itw-creative-works/backend-manager.git"
  },
  "keywords": [
    "cli",
    "backend manager",
    "firebase"
  ],
  "author": "ITW Creative Works",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itw-creative-works/backend-manager/issues"
  },
  "homepage": "https://itwcreativeworks.com",
  "preparePackage": {
    "input": "./src_",
    "output": "./dist_",
    "replace": {}
  },
  "dependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "@google-cloud/pubsub": "^5.2.2",
    "@google-cloud/storage": "^7.18.0",
    "@octokit/rest": "^19.0.13",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^6.19.7",
    "body-parser": "^1.20.4",
    "busboy": "^1.6.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "fs-jetpack": "^5.1.0",
    "glob": "^11.1.0",
    "hcaptcha": "^0.2.0",
    "inquirer": "^8.2.7",
    "itwcw-package-analytics": "^1.0.8",
    "json5": "^2.2.3",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.23",
    "lowdb": "^1.0.0",
    "mailchimp-api-v3": "^1.15.0",
    "mime-types": "^2.1.35",
    "mocha": "^11.7.5",
    "moment": "^2.30.1",
    "nanoid": "^3.3.11",
    "node-fetch": "^2.7.0",
    "node-powertools": "^2.3.2",
    "npm-api": "^1.0.1",
    "paypal-server-api": "^2.0.14",
    "pushid": "^1.0.0",
    "shortid": "^2.2.17",
    "uid-generator": "^2.0.0",
    "uuid": "^9.0.1",
    "wonderful-fetch": "^1.3.4",
    "wonderful-log": "^1.0.7",
    "wonderful-version": "^1.3.2",
    "yaml": "^2.8.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "chai": "^5.3.3",
    "prepare-package": "^1.2.6",
    "sinon": "^21.0.1"
  },
  "peerDependencies": {
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.5"
  }
}
