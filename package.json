{
  "name": "backend-manager",
  "version": "4.0.10",
  "description": "Quick tools for developing Firebase functions",
  "main": "src/manager/index.js",
  "bin": {
    "bem": "./bin/bem",
    "bm": "./bin/bem"
  },
  "scripts": {
    "_test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "test": "./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "test:usage": "./node_modules/mocha/bin/mocha test/usage.js --timeout=10000",
    "test:payment-resolver": "./node_modules/mocha/bin/mocha test/payment-resolver.js --timeout=10000",
    "test:user": "./node_modules/mocha/bin/mocha test/user.js --timeout=10000",
    "start": "node src/manager/index.js",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "engines": {
    "node": "18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itw-creative-works/backend-manager.git"
  },
  "keywords": [
    "cli",
    "backend manager",
    "firebase"
  ],
  "author": "ITW Creative Works",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itw-creative-works/backend-manager/issues"
  },
  "homepage": "https://itwcreativeworks.com",
  "preparePackage": {
    "input": "./src_",
    "output": "./dist_",
    "replace": {}
  },
  "dependencies": {
    "@firebase/rules-unit-testing": "^2.0.7",
    "@google-cloud/storage": "^7.13.0",
    "@octokit/rest": "^19.0.13",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^6.19.7",
    "body-parser": "^1.20.3",
    "busboy": "^1.6.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "fs-jetpack": "^5.1.0",
    "glob": "^11.0.0",
    "hcaptcha": "^0.1.1",
    "inquirer": "^8.2.5",
    "itwcw-package-analytics": "^1.0.6",
    "json5": "^2.2.3",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lowdb": "^1.0.0",
    "mailchimp-api-v3": "^1.15.0",
    "mime-types": "^2.1.35",
    "mocha": "^8.4.0",
    "moment": "^2.30.1",
    "nanoid": "^3.3.7",
    "node-fetch": "^2.7.0",
    "node-powertools": "^1.5.9",
    "npm-api": "^1.0.1",
    "paypal-server-api": "^2.0.14",
    "pushid": "^1.0.0",
    "resolve-account": "^1.0.25",
    "semver": "^7.6.3",
    "shortid": "^2.2.16",
    "sizeitup": "^1.0.9",
    "uid-generator": "^2.0.0",
    "ultimate-jekyll-poster": "^1.0.2",
    "uuid": "^9.0.1",
    "wonderful-fetch": "^1.1.12",
    "wonderful-log": "^1.0.7",
    "yaml": "^2.6.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "prepare-package": "^1.1.13"
  }
}
